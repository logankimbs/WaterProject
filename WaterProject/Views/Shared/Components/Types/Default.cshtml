@{
}

@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" asp-route-projectType="" asp-route-pageNum="1">Home</a>

@foreach (string type in Model)
{
    <div class="mb-1">
        <a asp-controller="Home"
           type="button"
           asp-action="Index"
           asp-route-projectType="@type"
           asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedType == type ? "btn-primary" : "btn-outline-secondary" ) btn-sm">@type</a>

    </div>
}